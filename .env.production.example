# Production Environment Variables for Vercel Deployment
# Copy this to .env.production.local for local production testing

# AI Provider API Keys (Required for functionality)
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GOOGLE_API_KEY=your_google_gemini_api_key_here
XAI_API_KEY=your_xai_grok_api_key_here
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# Optional: OpenRouter integration
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Build optimizations (automatically set by Vercel)
NODE_ENV=production
NEXT_TELEMETRY_DISABLED=1

# Vercel-specific (automatically provided)
VERCEL=1
VERCEL_ENV=production
VERCEL_URL=your-app.vercel.app

# Optional: Analytics and monitoring
NEXT_PUBLIC_VERCEL_ANALYTICS_ID=your_analytics_id
SENTRY_DSN=your_sentry_dsn_for_error_tracking

# Security (recommended)
NEXTAUTH_SECRET=your_nextauth_secret_32_chars_min
NEXTAUTH_URL=https://your-domain.vercel.app

# Feature flags (optional)
SKIP_ENV_VALIDATION=true
ANALYZE=false